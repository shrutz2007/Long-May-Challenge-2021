      "source": [
        "def prime_checker(n):\n",
        "  is_prime=True\n",
        "  for i in range(2,n-1):\n",
        "    if(n%i==0):\n",
        "      is_prime=False\n",
        "      \n",
        "  if is_prime:\n",
        "    print(\"prime\")\n",
        "\n",
        "  else:\n",
        "    print(\"Not Prime\")\n",
        "\n",
        "\n",
        "n=int(input(\"Enter the number : \"))\n",
        "prime_checker(n)"
      ],
