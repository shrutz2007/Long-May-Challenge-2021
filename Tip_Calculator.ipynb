        "#Tip calculater\n",
        "print(\"Welcome to tip calculator.\")\n",
        "a=float(input(\"What was the total bill? $\"))\n",
        "b=int(input(\"what percent tip would yuo like to give? 10, 12 or 15?\"))\n",
        "n=int(input(\"How many people want to split the bill?\"))\n",
        "result=a*(1+b/100)\n",
        "bill=result/n\n",
        "print(round(bill,2))\n"
