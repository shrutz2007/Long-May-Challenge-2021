      "source": [
        "t= int(input())\n",
        "for i in range(t):\n",
        "     num=[int(x) for x in input().split(' ')]\n",
        "     a=min(num)\n",
        "     b=max(num)\n",
        "     multiply=a*b\n",
        "     while(b):\n",
        "       a,b=b,a%b\n",
        "     hcf=a\n",
        "     lcm=multiply/hcf  \n",
        "     print(hcf,int(lcm))14"
      ],
